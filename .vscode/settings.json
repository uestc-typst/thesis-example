{
    // 预览功能
    "tinymist.previewFeature": "enable",
    // 从不自动导出pdf文件
    "tinymist.exportPdf": "never",
    // "tinymist.exportPdf": "onSave",
    // 输出路径 以pdf结尾!
    "tinymist.outputPath": "${fileWorkspaceFolder}/学位论文写作指南及例子.pdf",
    // 根目录, 修改为example所在目录, 也是在引用图片等资源的根目录
    "tinymist.rootPath": "${fileWorkspaceFolder}",
    // 字体地址, 修改为模板下的fonts目录
    "tinymist.fontPaths": [
        "${fileWorkspaceFolder}/uestc-thesis-template/fonts"
    ],
}